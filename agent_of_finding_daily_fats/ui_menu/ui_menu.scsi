ui_menu_file_for_finding_daily_fats 
	<- 	ui_user_command_class_atom; 
		ui_user_command_class_view_kb;
	=> nrel_main_idtf: 
		[Сколько нужно грамм продукта, чтобы обеспечить дневную норму жиров?] (* <- lang_ru;; *);
		[How many of a product is nesessary for a day?] (* <- lang_en;; *);
	=> ui_nrel_command_template:
		[*
		question_finding_daily_fats _-> ._question_finding_daily_fats
			(*
				_-> ui_arg_1;;
			*);;
		._question_finding_daily_fats _<- question;;
		*];
	=> ui_nrel_command_lang_template: 
		[Запрос нахождения количества продукта для восполнения ежедневной необходимости жиров: $ui_arg_1] 
		(* <- lang_ru;; *);
		[Nesessary amount of product for daily fats $ui_arg_1] 
		(* <- lang_en;; *);;